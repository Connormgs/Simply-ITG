	<ActorFrame OnCommand="y,SCREEN_CENTER_Y+70"> <children>
		<BitmapText
			Font="_eurostile normal"
			Text="Total Actual"
			OnCommand="horizalign,left;x,SCREEN_CENTER_X-260;y,-24;zoom,0.6"
		/>
		<BitmapText
			Font="_futurist metalic"
			Text="@string.format( '%5.2f', 
				PROFILEMAN:GetMachineProfile():GetSongsActual(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_EASY)+
				PROFILEMAN:GetMachineProfile():GetSongsActual(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_MEDIUM)+
				PROFILEMAN:GetMachineProfile():GetSongsActual(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_HARD)+
				PROFILEMAN:GetMachineProfile():GetSongsActual(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_CHALLENGE)+
				PROFILEMAN:GetMachineProfile():GetCoursesActual(STEPS_TYPE_DANCE_DOUBLE,COURSE_DIFFICULTY_REGULAR)+
				PROFILEMAN:GetMachineProfile():GetCoursesActual(STEPS_TYPE_DANCE_DOUBLE,COURSE_DIFFICULTY_DIFFICULT)
				)"
			OnCommand="horizalign,right;x,SCREEN_CENTER_X+106;y,-24;zoom,0.7"
		/>
		<BitmapText
			Font="_eurostile normal"
			Text="Total Possible"
			OnCommand="horizalign,left;x,SCREEN_CENTER_X-260;y,-4;zoom,0.6"
		/>
		<BitmapText
			Font="_futurist metalic"
			Text="@string.format( '%5.2f', 
				PROFILEMAN:GetMachineProfile():GetSongsPossible(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_EASY)+
				PROFILEMAN:GetMachineProfile():GetSongsPossible(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_MEDIUM)+
				PROFILEMAN:GetMachineProfile():GetSongsPossible(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_HARD)+
				PROFILEMAN:GetMachineProfile():GetSongsPossible(STEPS_TYPE_DANCE_DOUBLE,DIFFICULTY_CHALLENGE)+
				PROFILEMAN:GetMachineProfile():GetCoursesPossible(STEPS_TYPE_DANCE_DOUBLE,COURSE_DIFFICULTY_REGULAR)+
				PROFILEMAN:GetMachineProfile():GetCoursesPossible(STEPS_TYPE_DANCE_DOUBLE,COURSE_DIFFICULTY_DIFFICULT)
				)"
			OnCommand="horizalign,right;x,SCREEN_CENTER_X+106;y,-4;zoom,0.7"
		/>
		<BitmapText
			Font="_eurostile normal"
			Text="Star Complete"
			OnCommand="horizalign,left;x,SCREEN_CENTER_X-260;y,+20;zoom,0.8"
		/>
		<BitmapText
			Font="_futurist metalic"
			Text="@FormatPercentScore( GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE))"
			OnCommand="%function (self) self:horizalign('right'); self:x(SCREEN_CENTER_X+136); self:y(20); self:zoom(1); if GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE) &gt;= 1 then self:diffuseshift() end; self:effectcolor1(0.5,0.7,1,1); end"
		/>
		<AutoActor
			File="@THEME:GetPath(EC_GRAPHICS,'ScreenEvaluation','grade ' .. GradeToString(GetGradeFromPercent(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE))))"
			OnCommand="x,SCREEN_CENTER_X+200;y,0;zoom,0.9"
		/>
	</children></ActorFrame>
	<ActorFrame Condition="GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE) &gt; 1" OnCommand="y,SCREEN_CENTER_Y+138"> <children>
		<BitmapText
			Font="_eurostile normal"
			Text="Extra Credit"
			OnCommand="zoom,0.6;horizalign,left;x,SCREEN_CENTER_X-260;y,-24;diffuseshift;effectcolor1,0.2,0.2,0.2,1;"
		/>
		<BitmapText
			Font="_futurist metalic"
			Text="@string.format( '%5.2f', math.max(0,GetExtraCredit(STEPS_TYPE_DANCE_DOUBLE)) )"
			OnCommand="zoom,0.7;horizalign,right;x,SCREEN_CENTER_X+106;y,-24;diffuseshift;effectcolor1,0.2,0.2,0.2,1"
		/>
		<ActorScroller
			UseScroller="1"
			SecondsPerItem="0"
			NumItemsToDraw="20"
			ItemPaddingStart="0"
			ItemPaddingEnd="0"
	 		TransformFunction="function(self,offset,itemIndex,numItems) self:y(offset*34); end"
			OnCommand="%function(self) self:x(SCREEN_CENTER_X); self:y(4); self:SetCurrentAndDestinationItem((self:GetNumChildren()-1)/2); end"
		>
			<children>
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 1" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy SingleDigitGreats')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 2" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak1000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 3" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak2000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 4" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak3000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 5" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Greats80Percent')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 6" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy FullComboGreats')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 7" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Greats90Percent')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 8" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy FullComboPerfects')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 9" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak4000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 10" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak5000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 11" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Greats100Percent')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 12" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy FullComboMarvelouses')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 13" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak6000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 14" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak7000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 15" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak8000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 16" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak9000Combo')" OnCommand="zoom,0.3" />
				<AutoActor Condition="scale(GetTotalPercentComplete(STEPS_TYPE_DANCE_DOUBLE),1.0,GetMaxPercentCompelte(),0,17) &gt;= 17" File="@THEME:GetPath(EC_GRAPHICS,'ScreenAward','trophy Peak10000Combo')" OnCommand="zoom,0.3" />
			</children>
		</ActorScroller>
	</children></ActorFrame>
</children></ActorFrame>