			File="@THEME:GetPath(EC_GRAPHICS,'ScreenRanking','CourseDifficulty difficult')" 
			OnCommand="%function(self) self:y( scale(7,1,7,SCREEN_CENTER_Y-150,SCREEN_CENTER_Y+10) ); end"
		/>
	</children></ActorFrame>
