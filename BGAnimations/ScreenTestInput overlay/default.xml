<ActorFrame><children>


<Layer
Type="Quad"
InitCommand="queuecommand,Pulse"
PulseCommand="%function(self)
self:sleep(0.05)
BroadcastMK6Sensors()
self:queuecommand('Pulse')
end"
/>
	
<!--P1 Pad -->
<ActorFrame OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" ><children>
<ActorFrame><children>
<AutoActor
File="floor"
/></children></ActorFrame>
